<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="daftarAlamatVM" type="com.bokusapplication.app.modules.daftaralamat.data.viewmodel.DaftarAlamatVM"/>
    </data>
    <FrameLayout android:id="@+id/frameDaftarAlamat"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_25pxv"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnuser"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowuser"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_39pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_39pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:id="@+id/txtHelloUseru"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:text="@{daftarAlamatVM.daftarAlamatModel.txtHelloUseru}"
                        style="@style/txtMulishromanbold16" tools:text="@string/msg_hello_user_u"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBIodata"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_57pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{daftarAlamatVM.daftarAlamatModel.txtBIodata}"
                style="@style/txtMulishromanbold20" tools:text="@string/lbl_biodata"/>
            <ImageView android:id="@+id/imageGroup"
                android:layout_height="@dimen/_113pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="@dimen/_113pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerDaftarAlamat"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_4pxh}" tools:itemCount="2" tools:listitem="@layout/row_daftar_alamat"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowjeniskelamin"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJenisKelamin"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.59"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{daftarAlamatVM.daftarAlamatModel.txtJenisKelamin}"
                    style="@style/txtMulishromanbold15" tools:text="@string/lbl_jenis_kelamin"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPria"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.18"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{daftarAlamatVM.daftarAlamatModel.txtPria}"
                    style="@style/txtMulishromanbold15" tools:text="@string/lbl_pria"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUbahTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{daftarAlamatVM.daftarAlamatModel.txtUbahTwo}"
                    style="@style/txtMulishromanbold15_1" tools:text="@string/lbl_ubah"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowemail"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.18"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{daftarAlamatVM.daftarAlamatModel.txtEmail}"
                    style="@style/txtMulishromanbold15" tools:text="@string/lbl_email"/>
                <LinearLayout android:id="@+id/linearRowemailOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_97pxh"
                    android:layout_weight="0.82"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmailOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.77"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{daftarAlamatVM.daftarAlamatModel.txtEmailOne}"
                        style="@style/txtMulishromanbold13" tools:text="@string/msg_someone_email_c"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUbahThree"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_weight="0.23"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{daftarAlamatVM.daftarAlamatModel.txtUbahThree}"
                        style="@style/txtMulishromanbold15_1" tools:text="@string/lbl_ubah"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRownotelp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNoTelp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{daftarAlamatVM.daftarAlamatModel.txtNoTelp}"
                    style="@style/txtMulishromanbold15" tools:text="@string/lbl_no_telp"/>
                <LinearLayout android:id="@+id/linearRowpassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_83pxh"
                    android:layout_weight="0.76"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.75"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{daftarAlamatVM.daftarAlamatModel.txtPassword}"
                        style="@style/txtMulishromanbold15" tools:text="@string/lbl_08123xxxxxxxxx"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUbahFour"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{daftarAlamatVM.daftarAlamatModel.txtUbahFour}"
                        style="@style/txtMulishromanbold15_1" tools:text="@string/lbl_ubah"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
