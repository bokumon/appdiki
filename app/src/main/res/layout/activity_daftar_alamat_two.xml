<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="daftarAlamatTwoVM" type="com.bokusapplication.app.modules.daftaralamattwo.data.viewmodel.DaftarAlamatTwoVM"/>
    </data>
    <LinearLayout android:id="@+id/linearDaftarAlamatT"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_25pxv"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnuser"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_118pxv"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_weight="0.88" android:layout_width="0dp" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowuser"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_39pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_39pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:id="@+id/txtHelloUseru"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:text="@{daftarAlamatTwoVM.daftarAlamatTwoModel.txtHelloUseru}"
                        style="@style/txtMulishromanbold16" tools:text="@string/msg_hello_user_u"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDaftarAlamat"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_57pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{daftarAlamatTwoVM.daftarAlamatTwoModel.txtDaftarAlamat}"
                style="@style/txtMulishromanbold20" tools:text="@string/lbl_daftar_alamat"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnusername"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_300_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUsername"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{daftarAlamatTwoVM.daftarAlamatTwoModel.txtUsername}"
                    style="@style/txtMulishromanbold15" tools:text="@string/lbl_username"/>
                <TextView android:id="@+id/txtJlGajahMada"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_width="match_parent"
                    android:text="@{daftarAlamatTwoVM.daftarAlamatTwoModel.txtJlGajahMada}"
                    style="@style/txtMulishromanbold10" tools:text="@string/msg_jl_gajah_mada2"/>
            </LinearLayout>
            <ImageView android:id="@+id/imagePlus"
                android:layout_gravity="end"
                android:layout_height="@dimen/_31pxh"
                android:layout_marginEnd="@dimen/_112pxh"
                android:layout_marginStart="@dimen/_112pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_31pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_plus"
                tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnPilihAlamat"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_247pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/lbl_pilih_alamat"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_pilih_alamat"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageCheckmark"
            android:layout_height="@dimen/_13pxh"
            android:layout_marginBottom="@dimen/_482pxv"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_176pxv"
            android:layout_width="@dimen/_18pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_checkmark"
            tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_weight="1.12" android:layout_width="0dp" style="@style/groupStylebluegray_1">
            <LinearLayout android:id="@+id/linearRowsearch"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnsearch"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_white_a700"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtEksplor"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{daftarAlamatTwoVM.daftarAlamatTwoModel.txtEksplor}"
                        style="@style/txtMulishromanbold13_1" tools:text="@string/lbl_eksplor"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnmenu"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtKontak"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{daftarAlamatTwoVM.daftarAlamatTwoModel.txtKontak}"
                        style="@style/txtMulishromanbold13_1" tools:text="@string/lbl_kontak"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnuserOne"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <ImageView android:id="@+id/imageUserOne"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_31pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_31pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_bluegray_900_31x31"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_bluegray_900_31x31"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtUserTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{daftarAlamatTwoVM.daftarAlamatTwoModel.txtUserTwo}"
                        style="@style/txtMulishromanbold13_2" tools:text="@string/lbl_user"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
